{
  "name": "ngx-libraries",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "yarn syncpack fix-mismatches && yarn syncpack set-semver-ranges",
    "version:start": "yarn changeset",
    "version:apply": "yarn changeset version",
    "version:publish": "npm run build:all && yarn changeset publish",
    "build:all": "ng build ngx-tools && ng build ngx-material && ng build ngx-service-worker && ng build ngx-forms && ng build ngx-forms-inputs",
    "link:ngx-tools": "npm run build -w @juulsgaard/ngx-tools && cd dist/ngx-tools && yalc push",
    "link:ngx-material": "npm run build -w @juulsgaard/ngx-material && cd dist/ngx-material && yalc push",
    "link:ngx-forms": "npm run build -w @juulsgaard/ngx-forms && cd dist/ngx-forms && yalc push",
    "link:ngx-forms-inputs": "npm run build -w @juulsgaard/ngx-forms-inputs && cd dist/ngx-forms-inputs && yalc push",
    "link:ngx-service-worker": "npm run build -w @juulsgaard/ngx-service-worker && cd dist/ngx-service-worker && yalc push",
    "storybook:ngx-tools": "ng run ngx-tools:storybook",
    "storybook:ngx-material": "ng run ngx-material:storybook",
    "storybook:ngx-forms": "ng run ngx-forms:storybook",
    "storybook:ngx-forms-inputs": "ng run ngx-forms-inputs:storybook",
    "storybook:ngx-service-worker": "ng run ngx-service-worker:storybook"
  },
  "workspaces": [
    "./projects/*"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "16.2.7",
    "@angular/cdk": "16.2.6",
    "@angular/common": "16.2.7",
    "@angular/core": "16.2.7",
    "@angular/forms": "16.2.7",
    "@angular/material": "16.2.6",
    "@angular/platform-browser": "16.2.7",
    "@angular/service-worker": "16.2.7",
    "@angular/router": "16.2.7",
    "@juulsgaard/data-sources": "1.11.7",
    "@juulsgaard/ngx-forms-core": "16.0.5",
    "@juulsgaard/rxjs-tools": "1.4.6",
    "@juulsgaard/ts-tools": "1.4.3",
    "fuse.js": "6.6.2",
    "dayjs": "1.11.10",
    "ngx-colors": "3.5.2",
    "ngx-mat-timepicker": "16.1.1",
    "rxjs": "7.8.1",
    "tslib": "2.5.0",
    "zone.js": "0.13.0",
    "tinycolor2": "1.6.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.2.4",
    "@angular/cli": "16.2.4",
    "@angular/compiler": "16.2.7",
    "@angular/compiler-cli": "16.2.7",
    "@changesets/cli": "2.26.2",
    "@storybook/addon-essentials": "7.4.5",
    "@storybook/addon-interactions": "7.4.5",
    "@storybook/addon-links": "7.4.5",
    "@storybook/angular": "7.4.5",
    "@storybook/blocks": "7.4.5",
    "@storybook/testing-library": "0.2.1",
    "@types/jasmine": "4.3.1",
    "@types/node": "18.15.3",
    "@types/tinycolor2": "1.4.3",
    "jasmine-core": "4.6.0",
    "karma": "6.4.2",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "ng-packagr": "16.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.0.9",
    "syncpack": "9.8.6",
    "ts-node": "10.9.1",
    "typescript": "5.0.4",
    "@angular-devkit/architect": "0.1602.4",
