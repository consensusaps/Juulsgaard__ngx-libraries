{
  "name": "ngx-libraries",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "yarn syncpack fix-mismatches && yarn syncpack set-semver-ranges",
    "version:start": "yarn changeset",
    "version:apply": "yarn changeset version",
    "version:publish": "npm run build:all && yarn changeset publish",
    "build:all": "ng build ngx-tools && ng build ngx-material && ng build ngx-service-worker && ng build ngx-forms && ng build ngx-forms-inputs",
    "link:ngx-tools": "npm run build -w @juulsgaard/ngx-tools && cd dist/ngx-tools && yalc push",
    "link:ngx-material": "npm run build -w @juulsgaard/ngx-material && cd dist/ngx-material && yalc push",
    "link:ngx-forms": "npm run build -w @juulsgaard/ngx-forms && cd dist/ngx-forms && yalc push",
    "link:ngx-forms-inputs": "npm run build -w @juulsgaard/ngx-forms-inputs && cd dist/ngx-forms-inputs && yalc push",
    "link:ngx-service-worker": "npm run build -w @juulsgaard/ngx-service-worker && cd dist/ngx-service-worker && yalc push",
    "storybook:ngx-tools": "ng run ngx-tools:storybook",
    "storybook:ngx-material": "ng run ngx-material:storybook",
    "storybook:ngx-forms": "ng run ngx-forms:storybook",
    "storybook:ngx-forms-inputs": "ng run ngx-forms-inputs:storybook",
    "storybook:ngx-service-worker": "ng run ngx-service-worker:storybook",
    "link:ngx-forms-core": "yalc add @juulsgaard/ngx-forms-core && yalc link @juulsgaard/ngx-forms-core",
    "unlink:ngx-forms-core": "yalc remove @juulsgaard/ngx-forms-core"
  },
  "workspaces": [
    "./projects/*"
  ],
  "private": true,
  "type": "module",
  "dependencies": {
    "@angular/animations": "18.1.2",
    "@angular/cdk": "18.1.2",
    "@angular/common": "18.1.2",
    "@angular/core": "18.1.2",
    "@angular/forms": "18.1.2",
    "@angular/material": "18.1.2",
    "@angular/platform-browser": "18.1.2",
    "@angular/router": "18.1.2",
    "@angular/service-worker": "18.1.2",
    "@ctrl/tinycolor": "4.1.0",
    "@juulsgaard/data-sources": "1.14.0",
    "@juulsgaard/ngx-forms-core": "18.0.1",
    "@juulsgaard/rxjs-tools": "1.7.4",
    "@juulsgaard/ts-tools": "1.6.3",
    "dayjs": "1.11.12",
    "fuse.js": "7.0.0",
    "ngx-colors": "3.6.0",
    "ngx-mat-timepicker": "18.0.0",
    "rxjs": "7.8.1",
    "tslib": "2.6.3",
    "zone.js": "0.14.8"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1801.2",
    "@angular-devkit/build-angular": "18.1.2",
    "@angular-devkit/core": "18.1.2",
    "@angular/cli": "18.1.2",
    "@angular/compiler": "18.1.2",
    "@angular/compiler-cli": "18.1.2",
    "@angular/platform-browser-dynamic": "18.1.2",
    "@babel/core": "7.24.9",
    "@changesets/cli": "2.27.7",
    "@storybook/addon-essentials": "8.2.6",
    "@storybook/addon-interactions": "8.2.6",
    "@storybook/addon-links": "8.2.6",
    "@storybook/addon-themes": "8.2.6",
    "@storybook/angular": "8.2.6",
    "@storybook/blocks": "8.2.6",
    "@storybook/testing-library": "0.2.2",
    "@types/jasmine": "5.1.4",
    "@types/node": "18.19.42",
    "jasmine-core": "5.2.0",
    "karma": "6.4.3",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "ng-packagr": "18.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "storybook": "8.2.6",
    "syncpack": "12.4.0",
    "ts-node": "10.9.2",
    "typescript": "5.5.4"
  }
}
