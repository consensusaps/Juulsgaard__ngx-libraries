    "@juulsgaard/ts-tools": "^1.6.1",
    "typescript": "~5.4.4"
  },
  "devDependencies": {
    "@angular/common": "17.3.3",
    "@angular/core": "17.3.3",
    "@angular/forms": "17.3.3",
    "@juulsgaard/ngx-forms-core": "17.1.0",
    "@juulsgaard/ngx-tools": "17.1.0",
    "@juulsgaard/ts-tools": "1.6.1",
