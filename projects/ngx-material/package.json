{
  "name": "@juulsgaard/ngx-material",
  "description": "A library adding Material inspired utilities and components",
  "repository": "git@github.com:Juulsgaard/ngx-libraries.git",
  "homepage": "https://github.com/Juulsgaard/ngx-libraries/tree/main/projects/ngx-material",
  "version": "17.0.0",
  "publishConfig": {
    "directory": "../../dist/ngx-material"
  },
  "main": "src/public-api.ts",
  "scripts": {
    "build": "ng build ngx-material",
    "prepublish": "ng build ngx-material",
    "publish": "cd ../../dist/ngx-material && npm publish",
    "storybook": "ng run ngx-material:storybook"
  },
  "exports": {
    "./styles/defaults": {
      "sass": "./styles/defaults.scss"
    },
    "./styles": {
      "sass": "./styles/index.scss"
    },
    ".": {
      "sass": "./styles/index.scss"
    }
  },
  "peerDependencies": {
    "@angular/cdk": "^17.2.0",
    "@angular/common": "^17.2.1",
    "@angular/core": "^17.2.1",
    "@angular/material": "^17.2.0",
    "@ctrl/tinycolor": "^4.0.3",
    "@juulsgaard/ngx-tools": "^17.0.0",
    "@juulsgaard/rxjs-tools": "^1.5.3",
    "@juulsgaard/ts-tools": "^1.5.3",
    "typescript": "~5.3.3"
  },
  "devDependencies": {
    "@angular/cdk": "17.2.0",
    "@angular/common": "17.2.1",
    "@angular/core": "17.2.1",
    "@angular/material": "17.2.0",
    "@ctrl/tinycolor": "4.0.3",
    "@juulsgaard/ngx-tools": "17.0.0",
    "@juulsgaard/rxjs-tools": "1.5.3",
    "@juulsgaard/ts-tools": "1.5.3"
  },
  "dependencies": {
    "tslib": "2.6.2"
  }
}
