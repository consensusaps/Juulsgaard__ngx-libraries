{
  "name": "@juulsgaard/ngx-forms-inputs",
  "description": "A library containing default input components for different input types",
  "repository": "git@github.com:Juulsgaard/ngx-libraries.git",
  "homepage": "https://github.com/Juulsgaard/ngx-libraries/tree/main/projects/ngx-forms-inputs",
  "version": "17.1.2",
  "publishConfig": {
    "directory": "../../dist/ngx-forms-inputs"
  },
  "main": "src/public-api.ts",
  "exports": {
    "./styles": {
      "sass": "./styles/index.scss"
    },
    ".": {
      "sass": "./styles/index.scss"
    }
  },
  "scripts": {
    "build": "ng build ngx-forms-inputs",
    "prepublish": "ng build ngx-forms-inputs",
    "publish": "cd ../../dist/ngx-forms-inputs && npm publish",
    "storybook": "ng run ngx-forms-inputs:storybook",
    "link:ngx-forms-core": "yalc add @juulsgaard/ngx-forms-core && yalc link @juulsgaard/ngx-forms-core",
    "unlink:ngx-forms-core": "yalc remove @juulsgaard/ngx-forms-core"
  },
  "peerDependencies": {
    "@angular/animations": "^17.3.3",
    "@angular/cdk": "^17.3.3",
    "@angular/common": "^17.3.3",
    "@angular/core": "^17.3.3",
    "@angular/forms": "^17.3.3",
    "@angular/material": "^17.3.3",
    "@juulsgaard/ngx-forms": "^17.1.6",
    "@juulsgaard/ngx-forms-core": "^17.1.5",
    "@juulsgaard/ngx-tools": "^17.1.2",
    "dayjs": "^1.11.10",
    "ngx-colors": "^3.5.3",
    "ngx-mat-timepicker": "^17.1.0",
    "typescript": "~5.4.4"
  },
  "devDependencies": {
    "@angular/animations": "17.3.3",
    "@angular/cdk": "17.3.3",
    "@angular/common": "17.3.3",
    "@angular/core": "17.3.3",
    "@angular/forms": "17.3.3",
    "@angular/material": "17.3.3",
    "@angular/platform-browser": "17.3.3",
    "@juulsgaard/ngx-forms": "17.1.6",
    "@juulsgaard/ngx-forms-core": "17.1.5",
    "@juulsgaard/ngx-tools": "17.1.2",
    "dayjs": "1.11.10",
    "ngx-colors": "3.5.3",
    "ngx-mat-timepicker": "17.1.0"
  },
  "dependencies": {
    "tslib": "2.6.2"
  }
}
